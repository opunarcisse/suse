<%= simple_form_for(@teamtask) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :type %>
    <%= f.input :dateline %>
    <%= f.input :timeline %>
    <%= f.input :taskable_id %>
    <%= f.input :taskable_type %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
